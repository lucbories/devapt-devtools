{
	"nodes":{
		"NodeA":{
			"host":"localhost",
			"is_master":true,
			"servers":{
				"NodeALocal8080":{
					"type":"restify",
					"port":8080,
					"protocole":"http",
					"middlewares":[],
					
					"security": {
						"authentication": {
							"enabled":true,
							"plugins":["file_users"]
						}
					},
					
					"workers":{
						"min":1,
						"max":10,
						"method":"roundrobin"
					}
				},
				
				"NodeALocal8081":{
					"type":"express",
					"port":8081,
					"protocole":"http",
					"middlewares":[],
					
					"security": {
						"authentication": {
							"enabled":false,
							"plugins":["file_users", "db_users"]
						}
					}
				}
			}
		},
		
		"NodeB":{
			"host":"localhost",
			"is_master":false,
			"servers":{
				"NodeBLocal7080":{
					"type":"restify",
					"port":7080,
					"protocole":"http",
					"middlewares":[],
					
					"security": {
						"authentication": {
							"enabled":true,
							"plugins":["file_users"]
						}
					}
				},
				
				"clusterB2":{
					"type":"cluster",
					"port":7081,
					"protocole":"http",
					"servers":["serverB21", "serverB22"]
				},
				
				"serverB21":{
					"type":"express",
					"port":7082,
					"protocole":"http",
					"middlewares":[],
					
					"security": {
						"authentication": {
							"enabled":true,
							"plugins":["file_users"]
						}
					}
				},
				
				"serverB22":{
					"type":"express",
					"port":7083,
					"protocole":"http",
					"middlewares":[],
					
					"security": {
						"authentication": {
							"enabled":true,
							"plugins":["file_users"]
						}
					}
				}
			}
		}
	}
}