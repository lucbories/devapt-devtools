{
	"services":{
		"devtools_store":{
			"type":"html_assets",
			"routes":[
				{
					"route":"/assets2",
					"directory":"./public/tutorial-1",
					"default_file":"index.html"
				},
				{
					"route":"/examples",
					"directory":"./public/devtools",
					"default_file":"react_counter.html"
				}
			]
		},
		
		"metrics_http":{
			"type":"metrics_http"
		},
		
		"metrics_host":{
			"type":"metrics_host"
		},
		
		"metrics_nodejs":{
			"type":"metrics_nodejs"
		},
		
		"metrics_bus":{
			"type":"metrics_bus"
		},
		
		"topology":{
			"type":"topology"
		},
		
		"logs":{
			"type":"logs"
		},
		
		"devtools_panel":{
			"type":"middleware",
			"routes":[
				{
					"route":"/home",
					"page_view":"metrics_tabs",
					"page_menubar":"default_menubar"
				},
				{
					"route":"/topology/logical",
					"mw_file":"js/topology/topology_logical_mw.js"
				},
				{
					"route":"/topology/physical",
					"mw_file":"js/topology/topology_physical_mw.js"
				},
				{
					"route":"/metrics",
					"mw_file":"js/metrics/metrics_mw.js"
				},
				{
					"route":"/logs",
					"mw_file":"js/logs/logs_mw.js"
				},
				{
					"route":"/messages",
					"mw_file":"js/messages/messages_mw.js"
				},
                
                
				{
					"route":"/store/config/all",
					"mw_file":"js/store/config/store_config_mw.js"
				},
				{
					"route":"/store/config/applications",
					"mw_file":"js/store/config/store_config_applications_mw.js"
				},
				{
					"route":"/store/config/resources",
					"mw_file":"js/store/config/store_config_resources_mw.js"
				},
				{
					"route":"/store/config/services",
					"mw_file":"js/store/config/store_config_services_mw.js"
				},
				{
					"route":"/store/config/nodes",
					"mw_file":"js/store/config/store_config_servers_mw.js"
				},
				{
					"route":"/store/config/modules",
					"mw_file":"js/store/config/store_config_modules_mw.js"
				},
				{
					"route":"/store/config/plugins",
					"mw_file":"js/store/config/store_config_plugins_mw.js"
				},
				{
					"route":"/store/config/security",
					"mw_file":"js/store/config/store_config_security_mw.js"
				},
				
				
				{
					"route":"/store/config/views",
					"mw_file":"js/store/config/resources/store_config_views_mw.js"
				},
				{
					"route":"/store/config/models",
					"mw_file":"js/store/config/resources/store_config_models_mw.js"
				},
				{
					"route":"/store/config/menus",
					"mw_file":"js/store/config/resources/store_config_menus_mw.js"
				},
				{
					"route":"/store/config/menubars",
					"mw_file":"js/store/config/resources/store_config_menubars_mw.js"
				},
				
				
				{
					"route":"/store/runtime",
					"mw_file":"js/store/runtime/store_runtime_mw.js"
				},
				
				
				{
					"route":"/logs",
					"mw_file":"js/logs/logs_mw.js"
				}
			]
		},
		
		"html_assets_1":{
			"type":"html_assets",
			"routes":[
				{
					"route":"/css/.*.css",
					"directory":"./public"
				},
				{
					"route":"/js",
					"directory":"public/assets/js"
				},
				{
					"route":"/img/.*.jpg",
					"directory":"./public"
				},
				{
					"route":"/plugins",
					"plugins":["Foundation-6", "Cytoscape"],
					"adirectory":"public/assets/plugins"
				}
			],
			"security":{
				"enabled":false,
				"authentication":{
					"enabled":false,
					"engines":[]
				},
				"authorization":{
					"enabled":false,
					"engines":[],
					"permissions":{
						"*":["*"]
					}
				}
			}
		},
		
		"rest_api_models_query_1":{ 
			"type":"rest_api_models_query",
			"routes":[
				{
					"route":"/crud"
				}
			]
		},
		
		"rest_api_models_modifier_1":{
			"type":"rest_api_models_modifier",
			"routes":[
				{
					"route":"/crud"
				}
			]
		},
		
		"rest_api_resources_query_1":{
			"type":"rest_api_resources_query",
			"routes":[
				{
					"route":"/connexions",
					"collection":"connexions"
				},
				{
					"route":"/connexions/:name",
					"collection":"connexions",
					"item":"name"
				},
				
				{
					"route":"/views",
					"collection":"views"
				},
				{
					"route":"/views/:name",
					"collection":"views",
					"item":"name"
				},
				
				{
					"route":"/models",
					"collection":"models"
				},
				{
					"route":"/models/:name",
					"collection":"models",
					"item":"name"
				},
				
				{
					"route":"/menubars",
					"collection":"menubars"
				},
				{
					"route":"/menubars/:name",
					"collection":"menubars",
					"item":"name"
				},
				
				{
					"route":"/menus",
					"collection":"menus"
				},
				{
					"route":"/menus/:name",
					"collection":"menus",
					"item":"name"
				}
			]
		},
		
		"svc4":{
			"type":"rest_api_resources_modifier"
		}
	}
}