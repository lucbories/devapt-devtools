{
	"application": {
		"models": {
			"MODEL_AUTH_USERS_GROUPS": {
				"driver": "PDO_MYSQL",
				"connexion": "cx_auth",
				"role_read": "ROLE_AUTH_USERS_READ",
				"role_create": "ROLE_AUTH_USERS_CREATE",
				"role_update": "ROLE_AUTH_USERS_UPDATE",
				"role_delete": "ROLE_AUTH_USERS_DELETE",
				"crud_table": "users",
				"engine": {
					"name": "users_groups",
					"source": "json"
				},
				"associations": {
					"roles": {
						"mode": "many_to_many",
						"model": "MODEL_AUTH_MANY_USERS_GROUPS",
						"left_key": "id_user",
						"right_model": "MODEL_AUTH_GROUPS",
						"right_key": "id_group",
						"right_fields": [
							"id_group",
							"label"
						]
					}
				},
				"fields": {
					"users_id_user": {
						"type": "integer",
						"label": "Id",
						"is_editable": "0",
						"is_visible": "0",
						"sql_is_primary_key": "1",
						"sql_is_expression": "0",
						"sql_column": "id_user"
					},
					"users_login": {
						"type": "string",
						"label": "Login",
						"is_editable": "1",
						"is_visible": "1",
						"sql_is_primary_key": "0",
						"sql_is_expression": "0",
						"sql_column": "login",
						"field_value.validate": "alphaALPHAnum_-"
					},
					"users_lastname": {
						"type": "string",
						"label": "Lastname",
						"placeholder": "Enter lastname",
						"is_editable": "1",
						"is_visible": "1",
						"sql_is_primary_key": "0",
						"sql_is_expression": "0",
						"sql_column": "lastname",
						"field_value.validate": "alphaALPHAnum_-space",
						"field_value.validate_error_label": "upper or lower alphanumeric and - and _ and space"
					},
					"users_firstname": {
						"type": "string",
						"label": "Firstname",
						"placeholder": "Enter firstname",
						"is_editable": "1",
						"is_visible": "1",
						"sql_is_primary_key": "0",
						"sql_is_expression": "0",
						"sql_column": "firstname"
					},
					"users_email": {
						"type": "email",
						"label": "Email",
						"is_editable": "1",
						"is_visible": "1",
						"sql_is_primary_key": "0",
						"sql_is_expression": "0",
						"sql_column": "email",
						"field_value.validate_error_label": "you should enter a valid email"
					}
				}
			}
		}
	}
}