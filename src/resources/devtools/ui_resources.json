{
	"views":{
		"devtools_resources_from_json_dropdown":{
			"type":"Dropdown",
			"settings":{
				"has_mode_hover":false,
				"has_mode_toggle":true,
				"button":{
					"has_hollow":false,
					"has_dropdown":true
				},
				"pane": {
					"style":"height:auto;width:40%;"
				},
				"view":"devtools_resources_from_json_container"
			},
			"state":{
				"label":"JSON source"
			},
			"children":{
				"devtools_resources_from_json_container":{
					"type":"VBox",
					"settings":{
					},
					"state":{
						"items":[{"view":"devtools_resources_from_json_buttons"}, {"view":"devtools_resources_from_json_textarea"}]
					},
					"children":{
						"devtools_resources_from_json_textarea":{
							"type":"TextArea",
							"settings":{
								"style":"min-height:200px;height:90%;"
							},
							"state":{
							},
							"children":{}
						},
						"devtools_resources_from_json_buttons":{
							"type":"HBox",
							"settings":{
								"sizes":[
									"small-4 large-3",
									"small-4 large-3",
									"small-4 large-3"
								]
							},
							"state":{
								"items":[{"view":"devtools_resources_from_json_clear"}, {"view":"devtools_resources_from_json_register"}, {"view":"devtools_resources_from_json_preview"}]
							},
							"children":{
								"devtools_resources_from_json_clear":{
									"type":"Anchor",
									"settings":{
										"class":"button",
										"href":"#ignore_nothing"
									},
									"state":{
										"label":"Clear",
										"bindings":{
											"emitter_jquery":[
												{
													"source_type":"jquery",
													"source_selector":"#devtools_resources_from_json_clear",
													"dom_event":"click",
													"transform":null,
													"target_types":["view"],
													"target_selectors":["devtools_resources_from_json_textarea"],
													"target_method":"clear_dom_value",
													"options":{}
												}
											]
										}
									}
								},
								"devtools_resources_from_json_register":{
									"type":"Anchor",
									"settings":{
										"class":"button",
										"href":"#ignore_nothing"
									},
									"state":{
										"label":"Register",
										"bindings":{
											"emitter_dom":[
												{
													"source_type":"delegate",
													"source_selector":"a.button",
													"dom_event":"click",
													"transform":null,
													"target_types":["view"],
													"target_selectors":["devtools_resources_preview"],
													"target_method":"register_from_json",
													"options":{
														"method": {
															"operands":{
																"json_source_view":"devtools_resources_from_json_textarea",
																"json_source_getter":"get_object_value"
															}
														}
													}
												},
												{
													"source_type":"delegate",
													"source_selector":"a.button",
													"dom_event":"click",
													"transform":null,
													"target_types":["view", "view", "view", "view"],
													"target_selectors":["devtools_resources_view_attributes", "devtools_resources_view_state", "devtools_resources_view_settings", "devtools_resources_view_children"],
													"target_method":"set_object_attributes",
													"options":{
														"method": {
															"operands":{
																"json_source_view":"devtools_resources_from_json_textarea",
																"json_source_getter":"get_object_value"
															}
														}
													}
												}
											]
										}
									}
								},
								"devtools_resources_from_json_preview":{
									"type":"Anchor",
									"settings":{
										"class":"button",
										"href":"#ignore_nothing"
									},
									"state":{
										"label":"Preview",
										"bindings":{
											"emitter_dom":[
												{
													"source_type":"delegate",
													"source_selector":"a.button",
													"dom_event":"click",
													"transform":null,
													"target_types":["view"],
													"target_selectors":["devtools_resources_preview"],
													"target_method":"register_and_render_inside_from_json",
													"options":{
														"method": {
															"operands":{
																"json_source_view":"devtools_resources_from_json_textarea",
																"json_source_getter":"get_object_value"
															}
														}
													}
												}
											]
										}
									}
								}
							}
						}
					}
				}
			}
		},
		"devtools_resources_list_dropdown":{
			"type":"Dropdown",
			"settings":{
				"has_mode_hover":true,
				"has_mode_toggle":false,
				"button":{
					"has_hollow":false,
					"has_dropdown":true
				},
				"view":"devtools_resources_list_content"
			},
			"state":{
				"label":"Application views"
			},
			"children":{
				"devtools_resources_list_content":{
					"type":"Container",
					"settings":{},
					"state":{
						"items":[{"view":"devtools_resources_filter"}, {"view":"devtools_resources_list"}]
					}
				},
				"devtools_resources_filter":{
					"type":"InputField",
					"settings":{
						"format":"string"
					},
					"state":{
						"label":null,
						"placeholder":"type a view filter",
						"bindings":{
							"streams":[
								{
									"source_stream":"keydown",
									"transform":null,
									"target_type":"view",
									"target_selector":"devtools_resources_list",
									"target_method":"ui_items_filter",
									"options":{
										"method":{
											"operands":["li"]
										}
									}
								}
							]
						}
					}
				},
				"devtools_resources_list":{
					"type":"Container",
					"rendering":"Drilldown",
					"settings":{},
					"state":{
						"label":"",
						"items":[],
						"headers":[["View name"]],
						"bindings":{
							"services":[
								{
									"service":"resources",
									"method":"devapt-res-list",
									"transform":{
										"result_type":"single",
										"order":"resources:asc",
										"fields":[
											{
												"name":"resources",
												"path":["resources_names"]
											}
										]
									},
									"state_path":["items"],
									"target_type":"view",
									"target_selector":"this",
									"target_method":"do_action_append",
									"options":{
										"method":{
											"collection":"views"
										}
									}
								}
							],
							"emitter_dom":[
								{
									"source_type":"delegate",
									"source_selector":"a",
									"dom_event":"click",
									"transform":{
										"result_type":"single",
										"fields":[
											{
												"name":"text",
												"path":["target", "textContent"]
											}
										]
									},
									"target_types":["view", "view", "view", "view", "view"],
									"target_selectors":["devtools_resources_preview", "devtools_resources_view_attributes", "devtools_resources_view_state", "devtools_resources_view_settings", "devtools_resources_view_children"],
									"target_methods":["render_inside_from_json", "set_view_attributes_by_name"]
								}
							]
						}
					}
				}
			}
		},
